<line indent=""><var>$z&#160;=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;get&#160;z&#160;position</text></line>
<line indent=""></line>
<line indent=""><var>$ware&#160;=&#160;</var><text>get&#160;next&#160;key:&#160;table=</text><var>$wares</var><text>&#160;key=</text><var>null</var></line>
<line indent=""><var>while&#160;</var><var>$ware</var></line>
<line interruptable="@" indent="&#160;"><var>=&#160;</var><text>wait&#160;</text><var>50</var><text>&#160;ms</text></line>
<line indent="&#160;"><var>$amount&#160;=&#160;</var><var>$wares</var><text>[</text><var>$ware</var><text>]</text></line>
<line indent="&#160;"><var>if&#160;not&#160;</var><var>$amount</var></line>
<line indent="&#160;&#160;"><var>$ware&#160;=&#160;</var><text>get&#160;next&#160;key:&#160;table=</text><var>$wares</var><text>&#160;key=</text><var>$ware</var></line>
<line indent="&#160;&#160;"><text>continue</text></line>
<line indent="&#160;"><text>end</text></line>
<line indent="&#160;"></line>
<line indent="&#160;"><var>if&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;get&#160;free&#160;amount&#160;of&#160;ware&#160;</text><var>$ware</var><text>&#160;in&#160;cargo&#160;bay</text></line>
<line indent="&#160;&#160;"><var>$added&#160;=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;add&#160;</text><var>$amount</var><text>&#160;units&#160;of&#160;</text><var>$ware</var></line>
<line indent="&#160;"><text>else</text></line>
<line indent="&#160;&#160;"><var>$added&#160;=&#160;</var><var>0</var></line>
<line indent="&#160;"><text>end</text></line>
<line indent="&#160;"></line>
<line indent="&#160;"><var>if&#160;</var><var>$added</var><text>&#160;</text><var>!=</var><text>&#160;</text><var>$amount</var></line>
<line indent=""><comment><text>*&#160;</text><var>Eject&#160;any&#160;wares&#160;that&#160;did&#160;not&#160;fit&#160;target&#160;cargo&#160;bay&#160;into&#160;space&#160;as&#160;flying&#160;wares.&#160;</var></comment></line>
<line indent="&#160;&#160;"><var>$amount&#160;=&#160;</var><var>$amount</var><text>&#160;</text><var>-</var><text>&#160;</text><var>$added</var></line>
<line interruptable="@" indent="&#160;&#160;"><var>=&#160;</var><var>[THIS]&#160;-&gt;</var><text>&#160;call&#160;script&#160;</text><call>!galaxyeditor.create.flyingware</call><text>&#160;:&#160;</text><text>&#160;ware=</text><var>$ware</var><text>&#160;</text><text>&#160;count=</text><var>$amount</var><text>&#160;</text><text>&#160;x=</text><var>$x</var><text>&#160;</text><text>&#160;y=</text><var>$y</var><text>&#160;</text><text>&#160;z=</text><var>$z</var></line>
<line indent="&#160;"><text>end</text></line>
<line indent="&#160;"><var>$ware&#160;=&#160;</var><text>get&#160;next&#160;key:&#160;table=</text><var>$wares</var><text>&#160;key=</text><var>$ware</var></line>
<line indent=""><text>end</text></line>
<line indent=""></line>
<line indent=""><text>return&#160;</text><var>null</var></line>
</sourcetext>
