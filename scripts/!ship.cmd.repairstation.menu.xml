<script>
<name>!ship.cmd.repairstation.menu</name>
<version>3</version>
<engineversion>73</engineversion>
<description>Command Menu: REPAIR_STATION</description>
<arguments>
<argument index="1" name="a.obj" type="Value" desc="Object" />
<argument index="2" name="a.state" type="Var/String" desc="State" />
<argument index="3" name="a.value" type="Value" desc="Value" />
</arguments>
<sourceplaintext>
<![CDATA[
  $desc = read text: page=6020 id=249
  
  $cmd = COMMAND_DRONE_REPAIR_STATION
  
  if $a.state == 'start'
@   $menu = $a.obj -> call script !lib.input.cmd.target.player :  Command=$cmd  Ships=[TRUE]  a.stations=[TRUE]
    $sub = change datatype[$cmd] to DATATYPE_INT
    $text = read text: page=2010 id=$sub
    $input = create input: id='target' menu=$menu title=$text description=$desc callback=[FALSE]
    return $input
    
  else if $a.state == 'target' OR $a.state == 'target2'
    if $a.value == Station
      $input = create input: id='target2', user input=Var/Station, refobj=$a.obj, description=$desc, without sector=[FALSE], filterclass=null, racemask=null, flags=null, default=null
      return $input
    else if $a.value == Ship
      $input = create input: id='target2', user input=Var/Ship, refobj=$a.obj, description=$desc, without sector=[FALSE], filterclass=null, racemask=null, flags=null, default=null
      return $input
    else
      $a.obj ->set local variable: name='menu.target' value=$a.value
      return [TRUE]
    end
    
  else if $a.state == 'arguments'
    $target = $a.obj -> get local variable: name='menu.target'
    $ret = create new array, arguments=$target, null, null, null, null
    gosub sub.ClearVars
    return $ret
    
  else if $a.state == 'cancelled'
    if $a.value == 'target2'
@     $input = [THIS] -> call script !ship.cmd.repairstation.menu :  Object=$a.obj  State='start'  Value=null
      return $input
    end
    gosub sub.ClearVars
    return null
    
  else if $a.state == 'finished'
    gosub sub.ClearVars
  end
  
  return null
  
  sub.ClearVars:
  $a.obj ->set local variable: name='menu.target' value=null
  endsub
  return null
]]>
</sourceplaintext>
